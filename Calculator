        function RoundInputValue(val, ident, decimalPlace){
            document.getElementById(ident).value = Round(val, decimalPlace);
        }
        
        // The precision is how many decimal places you would like to round to. 0 being integers, 1 being 1 decimal place, etc.
        function Round(value, precision) {
            var multiplier = Math.pow(10, precision || 0);
            return Math.round(value * multiplier) / multiplier;
        }

        var testElement = document.getElementById('testing');
